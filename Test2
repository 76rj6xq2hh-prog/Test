#include <iostream>
using namespace std;

int main() {
    int num, sum = 0;
    cout << "Enter 5 numbers: ";
    for (int i = 0; i < 5; i++) {
        cin >> num;
        sum += num;
    }
    double average = sum / 5.0;
    cout << "Sum = " << sum << endl;
    cout << "Average = " << average << endl;

    if (average >= 75)
        cout << "You passed!" << endl;
    else
        cout << "You failed!" << endl;
    return 0;
}
